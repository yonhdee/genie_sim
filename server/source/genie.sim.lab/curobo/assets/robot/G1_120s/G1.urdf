    <origin xyz="0 0 0.6485" rpy="0 0 0"/>  <!-- 从底盘上方34.85cm处开始 -->
    <parent link="base_link"/>
    <child link="link-up-down_body"/>
    <axis xyz="0 0 1"/>                      <!-- 沿Z轴方向运动 -->
    <limit lower="0" upper="0.5" effort="1000" velocity="2.6100"/>  <!-- 升降范围0-30cm -->
  </joint>

